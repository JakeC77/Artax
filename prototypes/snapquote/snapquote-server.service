[Unit]
Description=SnapQuote API Server
After=network.target

[Service]
Type=simple
User=openclaw
WorkingDirectory=/home/openclaw/.openclaw/workspace/artax/prototypes/snapquote
Environment=PATH=/home/openclaw/.openclaw/workspace/artax/prototypes/snapquote/.venv/bin:/usr/bin
EnvironmentFile=/home/openclaw/.openclaw/workspace/artax/prototypes/snapquote/.env
ExecStart=/home/openclaw/.openclaw/workspace/artax/prototypes/snapquote/.venv/bin/uvicorn app.main:app --host 127.0.0.1 --port 8080
Restart=always
RestartSec=5

[Install]
WantedBy=multi-user.target
